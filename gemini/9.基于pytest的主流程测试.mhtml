<span class="hljs-keyword">import</span> subprocess  <span class="hljs-comment"># 用于执行 shell 命令 (如 allure)</span>
<span class="hljs-keyword">import</span> requests
<span class="hljs-keyword">from</span> pathlib <span class="hljs-keyword">import</span> Path
<span class="hljs-keyword">from</span> playwright.sync_api <span class="hljs-keyword">import</span> sync_playwright

<span class="hljs-comment"># --- 1. 配置 ---</span>

<span class="hljs-comment"># Allure 结果和报告的路径</span>
ALLURE_RESULTS_DIR = <span class="hljs-string">"./allure-results"</span>
ALLURE_REPORT_DIR = <span class="hljs-string">"./allure-report"</span> <span class="hljs-comment"># 静态HTML报告的输出目录</span>

<span class="hljs-comment"># Playwright 截图/PDF 的输出</span>
OUTPUT_PDF_PATH = <span class="hljs-string">"./allure_report.pdf"</span>
OUTPUT_IMAGE_PATH = <span class="hljs-string">"./allure_report_summary.png"</span>

<span class="hljs-comment"># 您要上传到的第三方系统 URL</span>
THIRD_PARTY_UPLOAD_URL = <span class="hljs-string">"https://api.third-party.com/upload"</span>
THIRD_PARTY_AUTH_TOKEN = <span class="hljs-string">"Bearer YOUR_SECRET_TOKEN"</span>


<span class="hljs-function"><span class="hljs-keyword">def</span> <span class="hljs-title">generate_static_report</span>():</span>
    <span class="hljs-string">"""
    步骤 1: 执行 allure generate，创建静态 HTML 报告
    """</span>
    print(<span class="hljs-string">f"正在从 <span class="hljs-subst">{ALLURE_RESULTS_DIR}</span> 生成静态 Allure 报告到 <span class="hljs-subst">{ALLURE_REPORT_DIR}</span>..."</span>)
    
    <span class="hljs-comment"># 使用 --clean 确保每次都生成最新的报告</span>
    command = [
        <span class="hljs-string">"allure"</span>,
        <span class="hljs-string">"generate"</span>,
        ALLURE_RESULTS_DIR,
        <span class="hljs-string">"-o"</span>,
        ALLURE_REPORT_DIR,
        <span class="hljs-string">"--clean"</span>
    ]
    
    <span class="hljs-keyword">try</span>:
        subprocess.run(command, check=<span class="hljs-literal">True</span>, shell=<span class="hljs-literal">True</span>)
        print(<span class="hljs-string">"静态 Allure 报告生成成功。"</span>)
    <span class="hljs-keyword">except</span> subprocess.CalledProcessError <span class="hljs-keyword">as</span> e:
        print(<span class="hljs-string">f"错误：Allure 报告生成失败: <span class="hljs-subst">{e}</span>"</span>)
        exit(<span class="hljs-number">1</span>)
    <span class="hljs-keyword">except</span> FileNotFoundError:
        print(<span class="hljs-string">"错误：找不到 'allure' 命令。请确保 Allure 命令行工具已安装并配置在系统 PATH 中。"</span>)
        exit(<span class="hljs-number">1</span>)

<span class="hljs-function"><span class="hljs-keyword">def</span> <span class="hljs-title">convert_report_to_files</span>():</span>
    <span class="hljs-string">"""
    步骤 2: 使用 Playwright 访问报告并生成 PDF 和图片
    """</span>
    print(<span class="hljs-string">"启动 Playwright 无头浏览器以转换报告..."</span>)
    
    <span class="hljs-comment"># 获取 allure-report/index.html 的绝对文件路径</span>
    report_path = Path(ALLURE_REPORT_DIR) / <span class="hljs-string">"index.html"</span>
    <span class="hljs-keyword">if</span> <span class="hljs-keyword">not</span> report_path.exists():
        print(<span class="hljs-string">f"错误：找不到 <span class="hljs-subst">{report_path}</span>。请先生成报告。"</span>)
        <span class="hljs-keyword">return</span>

    <span class="hljs-comment"># 使用 file:// 协议访问本地文件</span>
    report_url = <span class="hljs-string">f"file://<span class="hljs-subst">{report_path.resolve()}</span>"</span>

    <span class="hljs-keyword">with</span> sync_playwright() <span class="hljs-keyword">as</span> p:
        browser = p.chromium.launch(headless=<span class="hljs-literal">True</span>)
        page = browser.new_page()
        
        print(<span class="hljs-string">f"正在访问: <span class="hljs-subst">{report_url}</span>"</span>)
        page.goto(report_url)
        
        <span class="hljs-comment"># --- 关键：等待 Allure 的 JS 加载完毕 ---</span>
        <span class="hljs-comment"># 我们等待仪表盘上的 "summary" 元素出现</span>
        <span class="hljs-keyword">try</span>:
            page.wait_for_selector(<span class="hljs-string">"div.summary__item"</span>, timeout=<span class="hljs-number">10000</span>) <span class="hljs-comment"># 等待 10 秒</span>
            print(<span class="hljs-string">"Allure 仪表盘加载完成。"</span>)
        <span class="hljs-keyword">except</span> Exception <span class="hljs-keyword">as</span> e:
            print(<span class="hljs-string">f"警告：等待 Allure 页面加载超时，截图可能不完整。<span class="hljs-subst">{e}</span>"</span>)

        <span class="hljs-comment"># --- 生成 PDF ---</span>
        <span class="hljs-comment"># 打印成 PDF，scale=0.7 是为了让内容更适应A4页面</span>
        print(<span class="hljs-string">f"正在生成 PDF 文件到 <span class="hljs-subst">{OUTPUT_PDF_PATH}</span>..."</span>)
        page.pdf(path=OUTPUT_PDF_PATH, <span class="hljs-built_in">format</span>=<span class="hljs-string">"A4"</span>, scale=<span class="hljs-number">0.7</span>, print_background=<span class="hljs-literal">True</span>)

        <span class="hljs-comment"># --- 生成图片 (只截取仪表盘首页的可视区域) ---</span>
        print(<span class="hljs-string">f"正在生成 PNG 截图到 <span class="hljs-subst">{OUTPUT_IMAGE_PATH}</span>..."</span>)
        page.screenshot(path=OUTPUT_IMAGE_PATH)
        
        browser.close()
        print(<span class="hljs-string">"PDF 和图片生成完毕。"</span>)


<span class="hljs-function"><span class="hljs-keyword">def</span> <span class="hljs-title">upload_file_to_third_party</span>(<span class="hljs-params">file_path, file_type</span>):</span>
    <span class="hljs-string">"""
    步骤 3: 使用 requests 上传生成的文件
    """</span>
    print(<span class="hljs-string">f"准备上传文件: <span class="hljs-subst">{file_path}</span>"</span>)
    
    <span class="hljs-keyword">if</span> <span class="hljs-keyword">not</span> Path(file_path).exists():
        print(<span class="hljs-string">f"错误：要上传的文件 <span class="hljs-subst">{file_path}</span> 不存在。"</span>)
        <span class="hljs-keyword">return</span>

    <span class="hljs-comment"># 根据文件类型设置 MIME-Type</span>
    mime_type = <span class="hljs-string">'application/pdf'</span> <span class="hljs-keyword">if</span> file_type == <span class="hljs-string">'pdf'</span> <span class="hljs-keyword">else</span> <span class="hljs-string">'image/png'</span>
    file_name = Path(file_path).name

    <span class="hljs-comment"># 自定义请求头</span>
    headers = {
        <span class="hljs-string">'Authorization'</span>: THIRD_PARTY_AUTH_TOKEN,
        <span class="hljs-string">'User-Agent'</span>: <span class="hljs-string">'MyPytestUploader'</span>
    }

    <span class="hljs-keyword">try</span>:
        <span class="hljs-keyword">with</span> <span class="hljs-built_in">open</span>(file_path, <span class="hljs-string">'rb'</span>) <span class="hljs-keyword">as</span> f:
            <span class="hljs-comment"># 使用元组格式来指定文件名和 Content-Type</span>
            files = {
                <span class="hljs-string">'report_file'</span>: (file_name, f, mime_type)
            }
            <span class="hljs-comment"># 也可以在这里附加其他表单数据</span>
            data = {
                <span class="hljs-string">'project_name'</span>: <span class="hljs-string">'MyECommerceTest'</span>,
                <span class="hljs-string">'report_type'</span>: file_type
            }
            
            response = requests.post(
                THIRD_PARTY_UPLOAD_URL, 
                headers=headers, 
                files=files, 
                data=data
            )
            
            response.raise_for_status() <span class="hljs-comment"># 检查 4xx/5xx 错误</span>
            
            print(<span class="hljs-string">f"文件 <span class="hljs-subst">{file_name}</span> 上传成功! 状态码: <span class="hljs-subst">{response.status_code}</span>"</span>)
            print(<span class="hljs-string">f"服务器响应: <span class="hljs-subst">{response.json()}</span>"</span>)

    <span class="hljs-keyword">except</span> requests.exceptions.HTTPError <span class="hljs-keyword">as</span> e:
        print(<span class="hljs-string">f"上传失败! HTTP 错误: <span class="hljs-subst">{e.response.status_code}</span> <span class="hljs-subst">{e.response.text}</span>"</span>)
    <span class="hljs-keyword">except</span> requests.exceptions.RequestException <span class="hljs-keyword">as</span> e:
        print(<span class="hljs-string">f"上传失败! 请求发生错误: <span class="hljs-subst">{e}</span>"</span>)
    <span class="hljs-keyword">except</span> Exception <span class="hljs-keyword">as</span> e:
        print(<span class="hljs-string">f"发生未知错误: <span class="hljs-subst">{e}</span>"</span>)

<span class="hljs-comment"># --- 主执行流程 ---</span>
<span class="hljs-keyword">if</span> __name__ == <span class="hljs-string">"__main__"</span>:
    
    <span class="hljs-comment"># 1. (假设您已经运行了 pytest --alluredir=./allure-results)</span>
    
    <span class="hljs-comment"># 2. 生成静态 HTML 报告</span>
    generate_static_report()
    
    <span class="hljs-comment"># 3. 将 HTML 转换为 PDF 和图片</span>
    convert_report_to_files()
    
    <span class="hljs-comment"># 4. 上传 PDF</span>
    upload_file_to_third_party(OUTPUT_PDF_PATH, <span class="hljs-string">'pdf'</span>)
    
    <span class="hljs-comment"># 5. (可选) 上传图片</span>
    <span class="hljs-comment"># upload_file_to_third_party(OUTPUT_IMAGE_PATH, 'image')</span>
