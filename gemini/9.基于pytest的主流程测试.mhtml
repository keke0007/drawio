        <span class="hljs-keyword">if</span> <span class="hljs-string">"case_name"</span> <span class="hljs-keyword">in</span> row_data <span class="hljs-keyword">and</span> row_data[<span class="hljs-string">"case_name"</span>]:
            data_list.append(row_data)
            
    <span class="hljs-keyword">return</span> data_list

