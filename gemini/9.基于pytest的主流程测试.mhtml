    case_cache = request.config.cache.get(case_name, <span class="hljs-literal">None</span>)
    <span class="hljs-keyword">assert</span> case_cache <span class="hljs-keyword">is</span> <span class="hljs-keyword">not</span> <span class="hljs-literal">None</span>, <span class="hljs-string">f"用例 '<span class="hljs-subst">{case_name}</span>' 的缓存未找到"</span>
