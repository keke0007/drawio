<span class="hljs-keyword">import</span> openpyxl
<span class="hljs-keyword">import</span> json  <span class="hljs-comment"># 1. 导入 json 库</span>

<span class="hljs-comment"># ... (其他 fixture 如 api_client 保持不变) ...</span>

<span class="hljs-function"><span class="hljs-keyword">def</span> <span class="hljs-title">load_excel_data</span>(<span class="hljs-params">file_path, sheet_name</span>):</span>
    <span class="hljs-string">"""
    从Excel读取数据。
    如果发现名为 'request_payload' 的列，会尝试将其内容从JSON字符串解析为Python字典。
    """</span>
    workbook = openpyxl.load_workbook(filename=file_path)
    sheet = workbook[sheet_name]

    data_list = []
    headers = [cell.value <span class="hljs-keyword">for</span> cell <span class="hljs-keyword">in</span> sheet[<span class="hljs-number">1</span>]]

    <span class="hljs-keyword">for</span> row_index, row <span class="hljs-keyword">in</span> <span class="hljs-built_in">enumerate</span>(sheet.iter_rows(min_row=<span class="hljs-number">2</span>), start=<span class="hljs-number">2</span>):
        row_data = {}
        <span class="hljs-keyword">for</span> header, cell <span class="hljs-keyword">in</span> <span class="hljs-built_in">zip</span>(headers, row):
            row_data[header] = cell.value
        
        <span class="hljs-comment"># --- 核心改动在这里 ---</span>
        <span class="hljs-comment"># 2. 检查是否存在需要解析的列</span>
        <span class="hljs-keyword">if</span> <span class="hljs-string">"request_payload"</span> <span class="hljs-keyword">in</span> row_data:
            payload_str = row_data[<span class="hljs-string">"request_payload"</span>]
            
            <span class="hljs-comment"># 3. 尝试解析 JSON</span>
            <span class="hljs-keyword">if</span> payload_str <span class="hljs-keyword">and</span> <span class="hljs-built_in">isinstance</span>(payload_str, <span class="hljs-built_in">str</span>):
                <span class="hljs-keyword">try</span>:
                    <span class="hljs-comment"># 使用 json.loads() 将字符串转换为字典</span>
                    parsed_payload = json.loads(payload_str)
                    row_data[<span class="hljs-string">"request_payload"</span>] = parsed_payload
                <span class="hljs-keyword">except</span> json.JSONDecodeError:
                    <span class="hljs-comment"># 如果解析失败（例如，JSON格式错误）</span>
