    request.config.cache.<span class="hljs-built_in">set</span>(case_name, case_cache)

<span class="hljs-function"><span class="hljs-keyword">def</span> <span class="hljs-title">get_from_cache</span>(<span class="hljs-params">request, case_name, data_key</span>):</span>
