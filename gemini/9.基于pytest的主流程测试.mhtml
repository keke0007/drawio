<span class="hljs-keyword">for</span> f <span class="hljs-keyword">in</span> glob.glob(<span class="hljs-string">"./allure-results/*-result.json"</span>):
    <span class="hljs-keyword">with</span> <span class="hljs-built_in">open</span>(f) <span class="hljs-keyword">as</span> j:
        res = json.load(j)
        summary[<span class="hljs-string">"total"</span>] += <span class="hljs-number">1</span>
