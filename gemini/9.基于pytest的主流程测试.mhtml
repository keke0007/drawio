<span class="hljs-comment"># -------------------------------------------------------------------------</span>
<span class="hljs-comment"># 缓存帮助函数 (与之前相同)</span>
<span class="hljs-comment"># -------------------------------------------------------------------------</span>
<span class="hljs-function"><span class="hljs-keyword">def</span> <span class="hljs-title">save_to_cache</span>(<span class="hljs-params">request, case_name, data_key, data_value</span>):</span>
    case_cache = request.config.cache.get(case_name, {})
    case_cache[data_key] = data_value
    request.config.cache.<span class="hljs-built_in">set</span>(case_name, case_cache)

<span class="hljs-function"><span class="hljs-keyword">def</span> <span class="hljs-title">get_from_cache</span>(<span class="hljs-params">request, case_name, data_key</span>):</span>
    case_cache = request.config.cache.get(case_name, <span class="hljs-literal">None</span>)
    <span class="hljs-keyword">assert</span> case_cache <span class="hljs-keyword">is</span> <span class="hljs-keyword">not</span> <span class="hljs-literal">None</span>, <span class="hljs-string">f"用例 '<span class="hljs-subst">{case_name}</span>' 的缓存未找到"</span>
    <span class="hljs-keyword">assert</span> data_key <span class="hljs-keyword">in</span> case_cache, <span class="hljs-string">f"缓存中未找到 '<span class="hljs-subst">{data_key}</span>'"</span>
    <span class="hljs-keyword">return</span> case_cache[data_key]

<span class="hljs-comment"># -------------------------------------------------------------------------</span>
<span class="hljs-comment"># Allure 帮助函数：用于添加 API 请求和响应附件</span>
<span class="hljs-comment"># -------------------------------------------------------------------------</span>
<span class="hljs-function"><span class="hljs-keyword">def</span> <span class="hljs-title">attach_api_details</span>(<span class="hljs-params">name, response, payload=<span class="hljs-literal">None</span></span>):</span>
    <span class="hljs-string">"""为 Allure 报告添加详细的 API 请求和响应信息"""</span>
    <span class="hljs-keyword">with</span> allure.step(<span class="hljs-string">f"API Details: <span class="hljs-subst">{name}</span>"</span>):
        <span class="hljs-comment"># 附件1: 请求详情</span>
        <span class="hljs-keyword">if</span> payload:
            allure.attach(
                json.dumps(payload, indent=<span class="hljs-number">2</span>, ensure_ascii=<span class="hljs-literal">False</span>),
                name=<span class="hljs-string">"Request Payload"</span>,
                attachment_type=allure.attachment_type.JSON
            )
        allure.attach(
            <span class="hljs-string">f"URL: <span class="hljs-subst">{response.request.url}</span>\nMethod: <span class="hljs-subst">{response.request.method}</span>\nHeaders: <span class="hljs-subst">{response.request.headers}</span>"</span>,
            name=<span class="hljs-string">"Request Info"</span>,
            attachment_type=allure.attachment_type.TEXT
        )
        
        <span class="hljs-comment"># 附件2: 响应详情</span>
        allure.attach(
            <span class="hljs-string">f"Status Code: <span class="hljs-subst">{response.status_code}</span>\nHeaders: <span class="hljs-subst">{response.headers}</span>"</span>,
            name=<span class="hljs-string">"Response Info"</span>,
            attachment_type=allure.attachment_type.TEXT
        )
        <span class="hljs-keyword">try</span>:
            <span class="hljs-comment"># 尝试将响应体作为JSON附件</span>
            response_body = json.dumps(response.json(), indent=<span class="hljs-number">2</span>, ensure_ascii=<span class="hljs-literal">False</span>)
            allure.attach(
                response_body,
                name=<span class="hljs-string">"Response Body"</span>,
                attachment_type=allure.attachment_type.JSON
            )
        <span class="hljs-keyword">except</span> json.JSONDecodeError:
            <span class="hljs-comment"># 如果响应不是JSON，作为TEXT附件</span>
            allure.attach(
                response.text,
                name=<span class="hljs-string">"Response Body (Not JSON)"</span>,
                attachment_type=allure.attachment_type.TEXT
            )

