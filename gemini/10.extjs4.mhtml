                <span class="hljs-attr">tbar</span>: [
                    {
                        <span class="hljs-attr">text</span>: <span class="hljs-string">'添加条目'</span>,
                        <span class="hljs-attr">iconCls</span>: <span class="hljs-string">'x-tbar-add'</span>, <span class="hljs-comment">// ExtJS 默认的 'add' 图标</span>
                        <span class="hljs-attr">handler</span>: <span class="hljs-function"><span class="hljs-keyword">function</span>(<span class="hljs-params"></span>) </span>{
