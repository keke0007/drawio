<span class="hljs-keyword">PARTITION</span> <span class="hljs-keyword">BY</span> <span class="hljs-keyword">RANGE</span> (sale_year) <span class="hljs-comment">-- 主分区：按年</span>
SUBPARTITION <span class="hljs-keyword">BY</span> LIST (sale_month) <span class="hljs-comment">-- 子分区：按月</span>
(
    <span class="hljs-keyword">PARTITION</span> p_2024 <span class="hljs-keyword">VALUES</span> LESS THAN (<span class="hljs-number">2025</span>) (
        SUBPARTITION p_2024_q1 <span class="hljs-keyword">VALUES</span> (<span class="hljs-number">1</span>, <span class="hljs-number">2</span>, <span class="hljs-number">3</span>),
