l,1,null,null,1,1]];mutable:true"><span class="mat-mdc-button-persistent-ripple mdc-icon-button__ripple"></span><mat-icon _ngcontent-ng-c2829442937="" role="img" fonticon="content_copy" class="mat-icon notranslate google-symbols mat-ligature-font mat-icon-no-color" aria-hidden="true" data-mat-icon-type="font" data-mat-icon-name="content_copy"></mat-icon><span class="mat-focus-indicator"></span><span class="mat-mdc-button-touch-target"></span></button><!----><!----></div><!----></div><!----><div _ngcontent-ng-c2829442937="" class="formatted-code-block-internal-container ng-tns-c2829442937-89"><div _ngcontent-ng-c2829442937="" class="animated-opacity ng-tns-c2829442937-89"><pre _ngcontent-ng-c2829442937="" class="ng-tns-c2829442937-89"><code _ngcontent-ng-c2829442937="" role="text" data-test-id="code-content" class="code-container formatted ng-tns-c2829442937-89"><span class="hljs-keyword">CREATE</span> <span class="hljs-keyword">TABLE</span> departments (
