n;BardVeMetadataKey:[[&quot;r_ce23abdc1ebf6fcd&quot;,&quot;c_f088ae1d789b94ae&quot;,null,&quot;rc_44baa912950988a2&quot;,null,null,&quot;zh&quot;,null,1,null,null,1,0]];mutable:true"><span class="mat-mdc-button-persistent-ripple mdc-icon-button__ripple"></span><mat-icon _ngcontent-ng-c4227734975="" role="img" fonticon="content_copy" class="mat-icon notranslate google-symbols mat-ligature-font mat-icon-no-color" aria-hidden="true" data-mat-icon-type="font" data-mat-icon-name="content_copy"></mat-icon><span class="mat-focus-indicator"></span><span class="mat-mdc-button-touch-target"></span></button><!----><!----></div><!----></div><!----><div _ngcontent-ng-c4227734975="" class="formatted-code-block-internal-container ng-tns-c4227734975-295"><div _ngcontent-ng-c4227734975="" class="animated-opacity ng-tns-c4227734975-295"><pre _ngcontent-ng-c4227734975="" class="ng-tns-c4227734975-295"><code _ngcontent-ng-c4227734975="" role="text" data-test-id="code-content" class="code-container formatted ng-tns-c4227734975-295"><span class="hljs-comment">-- 假设这是你的源表</span>
<span class="hljs-keyword">CREATE</span> <span class="hljs-keyword">TABLE</span> raw_logs (
    request_id STRING,
    user_id STRING,
    log_message STRING,
