    };

    <span class="hljs-comment">// 4. 【重要】必须使用 execute 提交 FutureTask</span>
    <span class="hljs-comment">//    不能用 submit，submit 会返回一个新的 Future，而不是你创建的这个</span>
    executor.execute(futureTask);
}

<span class="hljs-comment">// 5. 从你的“完成队列”中按完成顺序获取结果</span>
<span class="hljs-keyword">for</span> (<span class="hljs-keyword">int</span> i = <span class="hljs-number">0</span>; i &lt; taskCount; i++) {
    Future&lt;String&gt; completedFuture = completionQueue.take(); <span class="hljs-comment">// 阻塞，直到有任务完成</span>
    String result = completedFuture.get();
    System.out.println(result);
}

