    <span class="hljs-keyword">int</span> taskId = i;
    
    <span class="hljs-comment">// 1. 使用 supplyAsync 提交任务</span>
    CompletableFuture.supplyAsync(() -&gt; {
        <span class="hljs-comment">// 任务逻辑</span>
        <span class="hljs-keyword">long</span> sleepTime = (<span class="hljs-keyword">long</span>) (Math.random() * <span class="hljs-number">5000</span>);
        Thread.sleep(sleepTime);
        <span class="hljs-keyword">return</span> <span class="hljs-string">"CF 任务 "</span> + taskId + <span class="hljs-string">" 完成"</span>;
