;track:generic_click,impression;BardVeMetadataKey:[[&quot;r_dc2de1389f061b0a&quot;,&quot;c_1fba4e99c93fa02e&quot;,null,&quot;rc_106f10d5131564cc&quot;,null,null,&quot;zh&quot;,null,1,null,null,1,0]];mutable:true"><span class="mat-mdc-button-persistent-ripple mdc-icon-button__ripple"></span><mat-icon _ngcontent-ng-c4272392036="" role="img" fonticon="content_copy" class="mat-icon notranslate google-symbols mat-ligature-font mat-icon-no-color" aria-hidden="true" data-mat-icon-type="font" data-mat-icon-name="content_copy"></mat-icon><span class="mat-focus-indicator"></span><span class="mat-mdc-button-touch-target"></span></button><!----><!----></div><!----></div><!----><div _ngcontent-ng-c4272392036="" class="formatted-code-block-internal-container ng-tns-c4272392036-103"><div _ngcontent-ng-c4272392036="" class="animated-opacity ng-tns-c4272392036-103"><pre _ngcontent-ng-c4272392036="" class="ng-tns-c4272392036-103"><code _ngcontent-ng-c4272392036="" role="text" data-test-id="code-content" class="code-container formatted ng-tns-c4272392036-103">ExecutorService executor = Executors.newFixedThreadPool(<span class="hljs-number">10</span>);
